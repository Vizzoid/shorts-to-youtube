{
  "name": "shorts-to-video",
  "version": "0.0.1",
  "description": "Press extension to open the YouTube video link for a Shorts video",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "ShortsToYouTube",
    "default_popup": "popup.html",
    "browser_style": true
  },
  "icons": {
    "48": "icon-48.png"
  },
  "permissions": [
    "tabs"
  ]
}
